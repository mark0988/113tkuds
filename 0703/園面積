import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        
        Scanner scanner = new Scanner(System.in);
        double r = scanner.nextDouble();
        
        
        double area = 3.14 * r * r;
        
       
        System.out.printf("%.2f\n", area);
        
        
        scanner.close();
    }
}
